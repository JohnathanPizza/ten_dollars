; vr5 num

>BRK_DATA_LOAD
	LDA.Z	VR5
	JSR	PAGE_TABLE_GET_DATA
	CMP.I	PAGE_COUNT_TOTAL
	BEQ	_BRK_DATA_LOAD_FAIL
	LDY	PROC_ACTIVE_ID
	LDA.Y	PROC_ACTIVE_DAP_ID
	JSR	PAGE_TABLE_GET_DATA
	CMP.I	PAGE_COUNT_TOTAL
	BEQ	_BRK_DATA_LOAD_SKIP
	JSR	PAGE_MAP_FIND
	JSR	PAGE_STORE_DAP
>_BRK_DATA_LOAD_SKIP
	LDA.Z	VR5
	STA.Y	PROC_ACTIVE_DAP_ID
	JSR	PAGE_TABLE_GET_DATA
	JSR	PAGE_MAP_FIND
	JSR	PAGE_LOAD_DAP
>_BRK_DATA_LOAD_FAIL
	JMP	PROC_UNPAUSE
