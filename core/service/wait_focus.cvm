>SERVICE_WAIT_FOCUS
	LDY	PROC_ACTIVE_ID		; load active id
	CMP	IO_READ_FOCUS		; compare to focus
	BEQ	_SERVICE_WAIT_FOCUS_GOOD	; if equal then unblock
	JMP	SERVICE_CYCLE_BEGIN	; start next cycle
>_SERVICE_WAIT_FOCUS_GOOD
	LDA.I	PROC_STATE_READY	; load ready value
	STA.Y	PROC_STATE		; update state
	JMP	SERVICE_CYCLE_BEGIN	; start next cycle
