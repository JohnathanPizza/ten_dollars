=SHELL_PTROFFL	0
=SHELL_PTROFFH	1

>SHELL_START
	LDA.I	BC_PTROFF
	BRK
	LDA.Z	VR11
	STA.Z	SHELL_PTROFFL
	LDA.Z	VR11 1
	STA.Z	SHELL_PTROFFH

	LDA.I	BC_SUSPEND
	BRK

	LDA.I	BC_WRITE_CLAIM
	BRK

	CLC
	LDA.I	SHELL_STR1
	ADC.Z	SHELL_PTROFFL
	STA.Z	VR0
	LDA.I	SHELL_STR1 > 8
	ADC.Z	SHELL_PTROFFH
	STA.Z	VR0 1
	JSR	PRINTL

	LDA.I	BC_WRITE_RELEASE
	BRK

	LDA.I	BC_EXIT
	BRK

>SHELL_STR1
"whats good drake\0

>SHELL_END
